<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Firma.Intranet</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Firma.Intranet.styles.css" asp-append-version="true" />
</head>
<body>
    <ul class="nav justify-content-center">
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Home" asp-action="Index">DashBoard 📊</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Home" asp-action="Pracownicy">Pracownicy 🧑🏼‍🍳</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Home" asp-action="EdytujMenu">Edycja Menu 🥨</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Home" asp-action="Zamowienia">Zamówienia 📋</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Strona" asp-action="Index">Index Test🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Pracownik" asp-action="Index">Pracownik Test 🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Danie" asp-action="Index">Danie Test 🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Urlop" asp-action="Index">Urlop Test 🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="MetodaPlatnosci" asp-action="Index">MetodaPlatnosci Test 🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Kategoria" asp-action="Index">Kategoria Test 🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="WiadomoscKontaktowa" asp-action="Index">WiadomoscKontaktowa Test 🛸</a>
        </li>
        <li class="nav-item">
            <a class="nav-link  fs-5" asp-controller="Zamowienie" asp-action="Index">Zamowienie Test 🛸</a>
        </li>

    </ul>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    @* <footer class="border-top footer text-muted"> *@
    @*     <div class="container text-center"> *@
    @*         &copy; 2025 - JSenderowski *@
    @*     </div> *@
    @* </footer> *@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
